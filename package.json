{
    "name": "@oat-sa/tao-item-runner",
    "version": "0.3.0",
    "displayName": "TAO Item Runner",
    "description": "TAO Item Runner modules",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "test": "npx qunit-testrunner -v",
        "test:keepAlive": "npx qunit-testrunner --keepalive",
        "test:cov": "npx qunit-testrunner --cov --coverage-instrument-spec=dist/**/*.js",
        "coverage": "nyc report",
        "coverage:html": "nyc report --reporter=lcov && open-cli coverage/lcov-report/index.html",
        "build": "rollup --config ./build/rollup.config.js",
        "build:watch": "rollup --config ./build/rollup.config.js --watch",
        "lint": "eslint src test",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/oat-sa/tao-item-runner-fe.git"
    },
    "keywords": [
        "tao",
        "item",
        "runner",
        "fe",
        "frontend"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/oat-sa/tao-item-runner-fe/issues"
    },
    "homepage": "https://github.com/oat-sa/tao-item-runner-fe#readme",
    "devDependencies": {
        "@oat-sa/tao-core-libs": "^0.1.0",
        "@oat-sa/tao-core-sdk": "^0.3.1",
        "@oat-sa/tao-qunit-testrunner": "^0.1.0",
        "eslint": "^5.16.0",
        "eslint-plugin-es": "^1.4.0",
        "eslint-plugin-jsdoc": "^8.0.1",
        "glob": "^7.1.4",
        "handlebars": "1.3.0",
        "http-server": "^0.11.1",
        "jquery": "1.9.1",
        "lodash": "2.4.1",
        "moment": "2.11.1",
        "node-qunit-puppeteer": "^1.0.12",
        "nyc": "^14.1.1",
        "open-cli": "^5.0.0",
        "promise-limit": "^2.7.0",
        "qunit": "^2.9.2",
        "require-css": "^0.1.10",
        "rollup": "^1.15.1",
        "rollup-plugin-alias": "^1.5.2"
    },
    "dependencies": {}
}
